<script>
(function(){
  const toc = document.getElementById('toc-links');
  const container = document.createElement('span');
  container.style.display = 'inline-block';
  container.style.marginLeft = '8px';

  // Collect headings (h2/h3) or figures (use alt as label)
  const items = [];
  const headings = Array.from(document.querySelectorAll('h2, h3'));
  headings.forEach((h,i) => {
    const id = h.id || ('sec-' + (i+1));
    h.id = id;
    items.push({el:h, label:h.textContent.trim() || ('Ενότητα ' + (i+1)), id});
  });

  if(items.length === 0) {
    // attempt to create pseudo-sections around tables
    const tables = Array.from(document.querySelectorAll('table'));
    tables.forEach((t,i) => {
      const id = t.id || ('tbl-' + (i+1));
      t.id = id;
      const label = t.getAttribute('aria-label') || ('Πίνακας ' + (i+1));
      items.push({el:t, label, id});
    });
  }

  if(items.length) {
    toc.textContent = '';
    items.forEach((it, idx) => {
      const a = document.createElement('a');
      a.href = '#' + it.id;
      a.textContent = it.label;
      a.style.marginRight = '10px';
      container.appendChild(a);
    });
    toc.appendChild(container);
  }

  // Back to top behavior
  const btn = document.getElementById('backToTop');
  const updateBtn = () => {
    btn.style.display = (window.scrollY > 240) ? 'block' : 'none';
  };
  btn.style.display = 'none';
  btn.addEventListener('click', () => window.scrollTo({top:0, behavior:'smooth'}));
  window.addEventListener('scroll', updateBtn, {passive:true});
  updateBtn();
})();
</script>


